<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Smurfline</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="main.css">
</head>
<body>
<div id="map"></div>
<svg id="world" version="1.1" width="800" height="500" xmlns="http://www.w3.org/2000/svg">
</svg>

<div id="text-container">
    <h2 id="date"></h2>
    <div id="test1"></div>
    <div id="test2"></div>
    <div id="test3"></div>
    <div id="test4"></div>
    <div id="test5"></div>
    <div id="test6"></div>
</div>
 <div class="container">
        <div class="am-pm"></div>
        <div class="digit hour-ten">
          <div class="top"></div>
          <div class="left-top"></div>
          <div class="right-top"></div>
          <div class="middle1"></div>
          <div class="middle2"></div>
          <div class="left-bottom"></div>
          <div class="right-bottom"></div>
          <div class="bottom"></div>
        </div>
         <div class="digit hour">
          <div class="top"></div>
          <div class="left-top"></div>
          <div class="right-top"></div>
          <div class="middle1"></div>
          <div class="middle2"></div>
          <div class="left-bottom"></div>
          <div class="right-bottom"></div>
          <div class="bottom"></div>
        </div>
        <div class="colon1"></div>
        <div class="colon2"></div>
        <div class="digit minute-ten">
          <div class="top"></div>
          <div class="left-top"></div>
          <div class="right-top"></div>
          <div class="middle1"></div>
          <div class="middle2"></div>
          <div class="left-bottom"></div>
          <div class="right-bottom"></div>
          <div class="bottom"></div>
        </div>
        <div class="digit minute">
          <div class="top"></div>
          <div class="left-top"></div>
          <div class="right-top"></div>
          <div class="middle1"></div>
          <div class="middle2"></div>
          <div class="left-bottom"></div>
          <div class="right-bottom"></div>
          <div class="bottom"></div>
        </div>
        <div class="colon3"></div>
        <div class="colon4"></div>
        <div class="digit second-ten">
          <div class="top"></div>
          <div class="left-top"></div>
          <div class="right-top"></div>
          <div class="middle1"></div>
          <div class="middle2"></div>
          <div class="left-bottom"></div>
          <div class="right-bottom"></div>
          <div class="bottom"></div>
        </div>
        <div class="digit second">
          <div class="top"></div>
          <div class="left-top"></div>
          <div class="right-top"></div>
          <div class="middle1"></div>
          <div class="middle2"></div>
          <div class="left-bottom"></div>
          <div class="right-bottom"></div>
          <div class="bottom"></div>
        </div>
      </div>
<script src="bundle.js" type="text/javascript"></script>

<script src="https://maps.googleapis.com/maps/api/js?key={{API_KEY}}&callback=initMap"
    async defer></script>
</body>
</html>